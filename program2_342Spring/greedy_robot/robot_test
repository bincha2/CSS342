echo "Test cases for Greedy Robot by David Kim - Dimpsey 342"
echo "Test X: (shortest distance, number of paths)"
echo "TEST 1: (2,2)"
./greedy_robot 1 3 4 4 3
echo "TEST 2: (Already there)"
./greedy_robot 2 1 1 1 1
echo "TEST 3: (NP, 0)"
./greedy_robot 1 1 1 5 1
echo "Testing custom test cases"
echo "Test 4: (straight line path, shortest distance: 5, number of paths)"
./greedy_robot 3 1 1 1 6
echo "Test 5: (no solution, shortest distance: NP, number of paths: 0)"
./greedy_robot 2 1 1 5 1
echo "Test 6: (large max_distance, shortest distance: 10, number of paths)"
./greedy_robot 5 0 0 3 3
echo "Test 7: (negative coordinates, shortest distance: 5, number of paths)"
./greedy_robot 3 -2 -2 -2 3
echo "Test 8: (horizontal path, shortest distance: 4, number of paths)"
./greedy_robot 3 -5 -5 5 -5
echo "Test 9: (vertical path, shortest distance: 4, number of paths)"
./greedy_robot 3 -5 -5 -5 5
echo "Test 10: number of paths = 34"
./greedy_robot 2 0 0 4 4
echo "TEST 11: (5, 5)"
echo "Shared testcases by classmates"
./greedy_robot 4 1 2 5 3 
echo "TEST 12: (3, 3)"
./greedy_robot 3 -2 -1 0 0 
echo "Test 13: (8, 16)"
./greedy_robot 2 2 0 5 5
echo "Test 14: (6, 2)"
./greedy_robot 1 2 2 5 5
 echo "Test 15: (5, 7)"
./greedy_robot 2 4 4 7 6
echo "Test 16: (3, 3)"
./greedy_robot 2 -2 -2 -4 -3
echo "Test 17: (6, 14)"
./greedy_robot 2 2 -1 -1 2
